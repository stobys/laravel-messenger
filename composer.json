{
    "name": "stobys/laravel-messenger",
    "description": "user messaging package for Laravel",
    "keywords": ["laravel", "messenger"],
    "license": "MIT",
    "authors": [
        {
            "name": "Slawomir Tobys",
            "email": "s.tobys@gmail.com",
            "homepage": "https://github.com/stobys",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/config": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "illuminate/database": "^5.5|^6.0|^7.0"
    },
    "require-dev": {
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.5",
        "orchestra/testbench": "^3.0|^4.0|^5.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "SylveK\\LaravelMessenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SylveK\\LaravelMessenger\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html build/coverage",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SylveK\\LaravelMessenger\\LaravelMessengerServiceProvider"
            ]
        }
    }
}
